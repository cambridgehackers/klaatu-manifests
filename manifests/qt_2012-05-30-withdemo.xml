<manifest>

<!-- Doesn't use ssh keys - read only copy -->
<remote name="CAMBRIDGEQT" fetch="git://gitorious.org/+cambridgehackers/qt/" />
<!-- Doesn't use ssh keys - read only copy -->
<remote name="gitorious" fetch="git://gitorious.org/cambridge/" />
<!-- used for ease of installation of vendor binaries, you can also get these from https://developers.google.com/android/nexus/drivers  -->
<remote name="PRIVATE_WORKAREA" fetch="ssh://leda/git/" />


<!-- Add busybox -->
<remote name="busybox-android"
        fetch="git://github.com/Gnurou/"/>
<project path="busybox-android" name="busybox-android" remote="busybox-android" revision="master"/>

<!-- Add the Klaatu services code -->
<project name="klaatu-services" path="external/klaatu-services" remote="gitorious" revision="master"/>

<!-- Add the klaatu tests code -->
<project name="klaatu-tests" path="external/klaatu-tests" remote="gitorious" revision="master"/>

<!-- Add the klaatu qmlscene code -->
<project name="klaatu-qmlscene" path="external/klaatu-qmlscene" remote="gitorious" revision="master"/>

<!-- Add vendor-specific binary drivers -->
<project path="vendor" name="bionicsf-vendor" remote="PRIVATE_WORKAREA" revision="4.1.2_maguro_JZO54K" />


<!-- Add the qt libs -->
<project path="external/cambridge-qtbase" name="cambridge-qtbase" remote="CAMBRIDGEQT" revision="bionicsf_2012-05-30"/>
<project path="external/cambridge-qtjsbackend" name="cambridge-qtjsbackend" remote="CAMBRIDGEQT" revision="bionicsf_2012-05-30"/>
<project path="external/cambridge-qtdeclarative" name="cambridge-qtdeclarative" remote="CAMBRIDGEQT" revision="bionicsf_2012-05-30"/>


<!-- Add the klaatu demo, if you can't reach this just use any qml file with klaatu-qmlscene for fun -->
<project path="external/klaatu-demos" name="klaatu-demos" remote="PRIVATE_WORKAREA" revision="master"/>


</manifest>
